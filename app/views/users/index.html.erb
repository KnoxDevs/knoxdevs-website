<h1>Users</h1>
<div class="container">
      <div class="row">
        <div class="col-md-2">
        	Email
        </div>
        <div class="col-md-1">
        	Admin
        </div>
      </div>

	<% @users.each do |user| %>

      <div class="row">
        <div class="col-md-2">
			<%= link_to user.email, user %>
        </div>
        <div class="col-md-1">
			<%= user.admin %>
        </div>
      </div>
	<% end %>
</div>